<Window x:Class="MyMailSender.TestSenderCredentials"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyMailSender"
        mc:Ignorable="d"
        Title="TestSenderCredentials" Height="140" Width="150">
    <StackPanel>
        <TextBlock Text="e-mail отправителя" HorizontalAlignment="Center"></TextBlock>
        <TextBox x:Name="tbSenderLogin" Text="rimedheart@yandex.ru" HorizontalAlignment="Center"></TextBox>
        <TextBlock Text="пароль отправителя" HorizontalAlignment="Center"></TextBlock>
        <TextBox x:Name="tbSenderPassword" HorizontalAlignment="Center" Width="140"></TextBox>
        <Button x:Name="btnEnterSenderCredentials" Content="OK" Margin="5" Click="btnEnterSenderCredentials_Click"></Button>
    </StackPanel>
</Window>
